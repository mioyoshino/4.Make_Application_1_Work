<h1>Books</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Body</th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>
<h2>New book</h2>
<%= form_for(@list, url: '/books') do |f| %>
 
    <div>Title</div>
    <%= f.text_field :title %>
 
    <div>Body</div>
    <%= f.text_area :body %>
 
    <%= f.submit 'Create Book' %>
 
<% end %>
</table>
